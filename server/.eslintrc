{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "object-curly-spacing": [
      "warn",
      "never"
    ],
    "func-names": "off",
    "import/prefer-default-export": "off",
    "space-before-function-paren": [
      "error",
      "never"
    ],
    "max-len": [
      "error",
      120,
      4
    ],
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "next"
      }
    ],
    "semi": "off"
  }
}